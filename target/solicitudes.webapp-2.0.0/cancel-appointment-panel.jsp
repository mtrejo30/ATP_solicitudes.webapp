<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>

<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />

<div id="cancelAppointmentPanelContainer" class="solicitud_container">
	<form
		id="cancel_appointment_form"
		method="post"
		target="postDocument">		
		<fieldset>
			<legend>Info</legend>

				<input id="appointment" name="appointment" type="hidden">
				<br/>
				<label id="cancelMotiveLabel"><spring:message code='Cancel_Motive' />:</label>
				<select id="cancelSolicitudId" name="cancelSolicitudId" ></select>

				<br/><br/>
		</fieldset>

	</form>

		<br/>		
		<div align="center" id="errorContainer" class="errorContainer"></div>

	<div align=center>
		<input id=cancelSolicitudAppointmentsButton type=button value="<spring:message code='Accept' />">
		<input id=cancelSolicitudButton type=button value="<spring:message code='Cancel' />">
	</div>
</div>
