                            <xs:attribute name="optional-pod1" type="xs:string" use="optional">
                                <xs:annotation>
                                    <xs:documentation>
                                        The first optional port of discharge for the container. (Field currently used only in
                                        the create/update appointment webservice request for Receive Export transaction types).
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
							<xs:attribute name="optional-pod1-name" type="xs:string" use="optional"/>
							<xs:attribute name="optional-pod2" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>
                                        The second optional port of discharge for the container. (Field currently used only in
                                        the create/update appointment webservice request for Receive Export transaction types).
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
							<xs:attribute name="optional-pod2-name" type="xs:string" use="optional"/>
						</xs:complexType>
					</xs:element>
