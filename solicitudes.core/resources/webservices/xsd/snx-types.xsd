                <xs:documentation>Required temperature setting (C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="o2-pct" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Percent of oxygen required for a refrigerated unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="co2-pct" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Percent of carbon dioxide needed for a refrigerated unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="humidity-pct" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Percent of humidity required for a refrigerated unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="vent-required-value" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Ventilation required for this unit.</xs:documentation>
