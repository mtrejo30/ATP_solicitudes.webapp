                    <xs:enumeration value="REQUEST_FOR_DEPARTURE"/>
                    <xs:enumeration value="ARRIVAL_ALLOWED"/>
                    <xs:enumeration value="DEPARTURE_ALLOWED"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="notes" use="optional">
            <xs:annotation>
                <xs:documentation>Any notes relevant to the train visit</xs:documentation>
