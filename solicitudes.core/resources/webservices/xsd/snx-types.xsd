        <xs:attribute name="line" type="xs:NCName" use="required">
            <xs:annotation>
                <xs:documentation>The line operator (shipping line) responsible for this Bill of
                    Lading
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="carrier-visit" type="xs:NCName" use="required">
            <xs:annotation>
                <xs:documentation>The vessel voyage call for this bill of lading</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="category" type="argo:tCategory" use="optional">
