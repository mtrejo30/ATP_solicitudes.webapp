    <xs:complexType name="tReeferRequirements">
        <xs:complexContent>
            <xs:annotation>
                <xs:documentation>Information specifiying refrigeration requirements and use for the
                    inventory unit.
                </xs:documentation>
            </xs:annotation>
            <xs:extension base="argo:tBaseReeferRequirements">
                <xs:attribute name="temp-min-c" type="xs:decimal" use="optional">
                    <xs:annotation>
                        <xs:documentation>Minimum allowed temperature (C)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="temp-max-c" type="xs:decimal" use="optional">
                    <xs:annotation>
                        <xs:documentation>Maximum allowed temperature (C)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="temp-display-unit" use="optional">
                    <xs:annotation>
                        <xs:documentation>System of units in which to display temperature.
                            C-Centigrade, F-Fahrenheit. (C is the default)
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="C"/>
                            <xs:enumeration value="F"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="is-power" type="xs:string" use="optional"/>
                <xs:attribute name="wanted-is-power" type="xs:string" use="optional"/>
                <xs:attribute name="time-latest-on-power" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>Time by which the Unit must be on-power to meet customer
                            requirements
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="time-requested-off-power" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>Time by which the Unit must be off-power to meet customer
                            requirements
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="time-requested" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>Time by which power is requested</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="time-monitor-1" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>1st time by which reefer should be monitored
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="time-monitor-2" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>2nd time by which reefer should be monitored
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="time-monitor-3" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>3rd time by which reefer should be monitored
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="time-monitor-4" type="xs:dateTime" use="optional">
                    <xs:annotation>
                        <xs:documentation>4th time by which reefer should be monitored
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="is-alarm-on" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation>Alarm is on (Y/N)</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="unplug-warn-min" type="xs:integer" use="optional">
                    <xs:annotation>
                        <xs:documentation>Number of minutes Reefer can be without power</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tReeferRecordingHistories">
        <xs:annotation>
            <xs:documentation>Information specifiying reefer recording history.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="reefer-recording-history" type="argo:tReeferRecordingHistory" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Details of Reefer Record History</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tReeferRecordingHistory">
        <xs:annotation>
            <xs:documentation>Details of Reefer Record History.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="time-of-recording" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Time of Recording</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="return-temp-c" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Return temperature (C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="vent-setting" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Ventilation setting</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="vent-unit" use="optional">
            <xs:annotation>
                <xs:documentation>Ventilation Unit (%, Cubic feet per minute, Cubic meters per hour)
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="PERCENTAGE"/>
                    <xs:enumeration value="CUBIC_FT_MIN"/>
                    <xs:enumeration value="CUBIC_M_HOUR"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="humidity-pct" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Humidity Percentage</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="o2-pct" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Percent of oxygen required for a refrigerated unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="co2-pct" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Percent of carbon dioxide needed for a refrigerated unit.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rec-fault-code" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Reefer Record fault code.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="max_mtr_tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Maximun Monitor temperature (C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="min_mtr_tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Minimum Monitor allowed temperature (C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="defrost-temp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Defrost temperature (C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="tmp-set-pt" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Temperature set point(C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="supply-tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Supply Temperature(C)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="sensor1-tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Temperature(C) read at sensor 1 inside the reefer container (USDA mandated).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="sensor2-tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Temperature(C) read at sensor 2 inside the reefer container (USDA mandated).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="sensor3-tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Temperature(C) read at sensor 3 inside the reefer container (USDA mandated).</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="sensor4-tmp" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Temperature(C) read at sensor 4 inside the reefer container (USDA mandated).</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="fuel-lvl" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Fuel Level Percentage</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="reefer-hours" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Reefer Hours</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="remark" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Remark</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="malfunc" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Container malfunctioned (Y/N)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="fan" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Fan setting (H - High; L - Low; A - Alt)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="drains" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>If drains are open (Y/N)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="is-bulb-on" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Bulb is on (Y/N)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="is-alarm-on" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Alarm is on (Y/N)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="tRouting">
        <xs:annotation>
            <xs:documentation>Routing specifies the carrier that brought or will bring the inventory
                unit to the facility and that which took it or will take it from the facility. Both
                intended and actual carriers are recorded.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="carrier" maxOccurs="4">
                <xs:annotation>
                    <xs:documentation>Details of a carrier, typically a vessel or train that brought
                        or will take the inventory unit to or from the facility.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="argo:tCarrier"/>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="pol" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Routing Point code for the port of loading</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="pol-name" type="xs:string"/>
        <xs:attribute name="pod-1" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Routing Point code for the 1st port of discharge
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="pod-1-name" type="xs:string"/>
        <xs:attribute name="pod-2" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Routing Point code for the 2nd port of discharge
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="pod-2-name" type="xs:string"/>
        <xs:attribute name="opl" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Routing Point code for the original port of loading
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="origin" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>The origin of the cargo - a free form String.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="destination" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>The destination of the cargo - a free form String.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="pin-number" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>PIN Number of the Unit..
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="return-to-location" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>The location to which the controlling line operator wants the consignee to bring the
                    container after stripping.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="designated-trucker" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Business unit id of the trucking company to which this unit will
                    be delivered.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="carrier-service" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Vessel or Train service that this Unit is intended to travel upon.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="group" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Code for a routing group to which this unit belongs
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="next-facility" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>For Units that will be going to another Facility in the Complex, this is the Facility
                    ID
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="tUnit">
        <xs:annotation>
            <xs:documentation>The Unit is the central object in SPARCS N4, representing a moveable
                item of inventory consisting of cargo and its packaging. The most obvious example
                being a container and its contents.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="equipment" type="argo:tUnitEquipment" minOccurs="0" maxOccurs="20"/>
            <xs:element name="position" type="argo:tPosition" minOccurs="0" maxOccurs="1"/>
            <xs:element name="routing" type="argo:tRouting" minOccurs="0" maxOccurs="1"/>
            <xs:element name="booking" type="argo:tBkg" minOccurs="0" maxOccurs="1"/>
            <xs:element name="edo" type="argo:tUnitEdo" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ero" type="argo:tUnitEro" minOccurs="0" maxOccurs="1"/>
            <xs:element name="elo" type="argo:tUnitElo" minOccurs="0" maxOccurs="1"/>
            <xs:element name="rail-order" type="argo:tUnitRailOrder" minOccurs="0" maxOccurs="1"/>
            <xs:element name="hazards" type="argo:tHazards" minOccurs="0" maxOccurs="1"/>
            <xs:element name="reefer" type="argo:tReeferRequirements" minOccurs="0" maxOccurs="1"/>
            <xs:element name="reefer-recording-histories" type="argo:tReeferRecordingHistories" minOccurs="0" maxOccurs="1"/>
            <xs:element name="oog" type="argo:tOog" minOccurs="0" maxOccurs="1"/>
            <xs:element name="handling" type="argo:tUnitHandling" minOccurs="0" maxOccurs="1"/>
            <xs:element name="contents" type="argo:tUnitContents" minOccurs="0" maxOccurs="1"/>
            <xs:element name="seals" type="argo:tUnitSeals" minOccurs="0" maxOccurs="1"/>
            <xs:element name="flags" type="argo:tFlags" minOccurs="0" maxOccurs="1"/>
            <xs:element name="unit-etc" type="argo:tUnitEtc" minOccurs="0" maxOccurs="1"/>
            <xs:element name="unit-flex" type="argo:tUnitFlexFields" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ufv-flex" type="argo:tUfvFlexFields" minOccurs="0" maxOccurs="1"/>
            <xs:element name="timestamps" type="argo:tUfvTimestamps" minOccurs="0" maxOccurs="1"/>
            <xs:element name="move-history" type="argo:tMoveHistory" minOccurs="0" maxOccurs="1"/>
            <xs:element name="non-move-history" type="argo:tNonMoveHistory" minOccurs="0" maxOccurs="1"/>
            <xs:element name="goods" type="argo:tGoods" minOccurs="0" maxOccurs="1"/>
            <xs:element name="observed-placards" type="argo:tObsPlacards" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="id" use="required">
            <xs:annotation>
                <xs:documentation>Identifier for the inventory unit, typically the container number.
                    Ignored for containerized units, as the equipment id of the primary equipment
                    will be used.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="unique-key" use="optional">
            <xs:annotation>
                <xs:documentation>A unique key to identify the inventory unit. Child records to the
                    Unit will be linked using this unique key.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="remarks" type="xs:string" use="optional"/>
        <xs:attribute name="category" type="argo:tCategory" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the category of the inventory unit.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="restow" type="argo:tRestowType" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the category of the inventory unit.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="import-delivery-order-id" type="xs:string" use="optional"/>
        <xs:attribute name="transit-state" use="optional">
            <xs:annotation>
                <xs:documentation>Code identifying the stage of the unit's visit to the facility. TO BE OMITTED WHEN USING SNX TO BRING UNITS INTO SYSTEM IN PRODUCTION MODE!
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="YARD"/>
                    <xs:enumeration value="ADVISED"/>
                    <xs:enumeration value="LOADED"/>
                    <xs:enumeration value="INBOUND"/>
                    <xs:enumeration value="ECIN"/>
                    <xs:enumeration value="ECOUT"/>
                    <xs:enumeration value="DEPARTED"/>
                    <xs:enumeration value="RETIRED"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="freight-kind" use="optional">
            <xs:annotation>
                <xs:documentation>Unit freight kind...FCL:laden container, MTY:empty container,
                    LCL:less-than-containerload, BBK:break-bulk unit
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="FCL"/>
                    <xs:enumeration value="MTY"/>
                    <xs:enumeration value="BBK"/>
                    <xs:enumeration value="LCL"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="line" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the Line Operator (Shipping Line)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="snx-update-note" use="optional">
            <xs:annotation>
                <xs:documentation>This note will be used in Event History Notes while update unit,if not specified default value is 'Unit updated through SNX'.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="agent1" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Associating Agent with Unit</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="agent2" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Associating Agent with Unit</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="is-ib-to-ob-move-direct" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>True if the discharge/load is a direct move to/from road truck.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="tVessel">
        <xs:annotation>
            <xs:documentation>Definition of an individual vessel.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="captain" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Name of the vessel's Captain</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="country-id" type="xs:NCName" use="optional">
            <xs:annotation>
                <xs:documentation>Flag under which the vessel is registered</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="id" type="xs:NCName" use="required">
            <xs:annotation>
                <xs:documentation>Identifying code for the vessel</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="lloyds-id" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>LLoyd's number</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="name" use="optional">
            <xs:annotation>
                <xs:documentation>Name of the vessel</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="notes" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Any notes relevant to the vessel</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="owner" type="xs:NCName" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the business that owns the vessel</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="radio-call-sign" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>The vessel's radio call sign</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="documentationNbr" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Documentation Number</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="serviceRegistryNbr" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Service Registry Number</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="stowage-scheme" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the stowage scheme that the vessel uses
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
