        <xs:attribute name="has-wheels" type="xs:string" use="optional"/>
        <xs:attribute name="is-permanently-out-of-service" type="xs:string" use="optional"/>
        <xs:attribute name="build-date" type="xs:date" use="optional"/>
        <xs:attribute name="cse-expiration" type="xs:string" use="optional"/>
