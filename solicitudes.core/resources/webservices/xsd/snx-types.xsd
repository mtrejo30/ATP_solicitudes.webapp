    <xs:complexType name="tUnitEquipment">
        <xs:complexContent>
            <xs:annotation>
                <xs:documentation>Item of equipment associated with the inventory unit - Container,
                    Chassis, Accessories.
                </xs:documentation>
            </xs:annotation>
            <xs:extension base="argo:tEquipment">
                <xs:sequence>
                    <xs:element name="damages" type="argo:tDamages" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Describes damages content of the inventory unit.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="flags" type="argo:tFlags" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Describes the assignment of a HOLD or grant of a PERMISSION at Unit Equipment
                                level.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="role" use="optional">
                    <xs:annotation>
                        <xs:documentation>Code for the role of this piece of equipment within the inventory unit.
                            For a containerized unit the Container will have the PRIMARY role. Each Unit must have
                            one and only one PRIMARY Equipment.
                            CARRIAGE- Equipment, i.e. chassis, used to tow the primary equipment
                            ACCESSORY- Accessory, such as a genset, attached to the Container
                            ACCESSORY_ON_CHS- Accessory attached to the chassis
                            PAYLOAD- Equipment, i.e. flatracks, bundled with the primary equipment
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="PRIMARY"/>
                            <xs:enumeration value="CARRIAGE"/>
                            <xs:enumeration value="ACCESSORY"/>
                            <xs:enumeration value="ACCESSORY_ON_CHS"/>
                            <xs:enumeration value="PAYLOAD"/>
                            <xs:enumeration value=""/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tEquipment">
        <xs:annotation>
            <xs:documentation>Item of equipment associated with the inventory unit - Container,
                Chassis, Accessories
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- 2007-11-20 rsatish v1.5.A ARGO-7656 moved to tUnitEquipment
            <xs:element name="damages" type="argo:tDamages" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Describes damages content of the inventory unit.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="flags" type="argo:tFlags" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Describes the assignment of a HOLD or grant of a PERMISSION at Unit Equipment
                        level.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            -->
            <xs:element name="physical" type="argo:tPhysical" minOccurs="0"/>
            <xs:element name="ownership" type="argo:tOwnership" minOccurs="0"/>
            <xs:element name="restrictions" type="argo:tRestrictions" minOccurs="0"/>
            <xs:element name="transponder" type="argo:tTransponder" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="eqid" use="required">
            <xs:annotation>
                <xs:documentation>Identifying number of the equipment (eg Container Number)
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="class" type="argo:tEquipmentClass" use="required">
            <xs:annotation>
                <xs:documentation>Class of the equipment</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="type" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the equipment type, for containers this is (usually) the
                    ISO code
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="created-by" type="xs:string"/>
        <xs:attribute name="created-date" type="xs:dateTime"/>
        <xs:attribute name="modified-by" type="xs:string"/>
        <xs:attribute name="modified-date" type="xs:dateTime"/>
        <xs:attribute name="tare-kg" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation>Tare weight of the equipment.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="height-mm" type="xs:integer" use="optional">
            <xs:annotation>
                <xs:documentation>Height of the equipment in millimeters</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="strength-code" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Strength code/designation (used by Matson)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="owner" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Code of the business unit that owns the equipment
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="operator" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Code of the business unit that is currently operating the equipment
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="material" type="argo:tMaterial" use="optional">
            <xs:annotation>
                <xs:documentation>The material the equipment is made of.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="eq-flex-01" type="xs:string" use="optional"/>
        <xs:attribute name="eq-flex-02" type="xs:string" use="optional"/>
        <xs:attribute name="eq-flex-03" type="xs:string" use="optional"/>
        <xs:attribute name="life-cycle-state" type="argo:tLifeCycleStateType" use="optional"/>
    </xs:complexType>

    <xs:complexType name="tFlags">
        <xs:annotation>
            <xs:documentation>Describes the assignment of a HOLD or grant of a PERMISSION.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="hold" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:attribute name="id" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>The id of the hold.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="permission" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:attribute name="id" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>The id of the permission.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tDamages">
        <xs:annotation>
            <xs:documentation>Encapsulates all damage to a piece of equipment.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="damage" type="argo:tDamage" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Details of one unit of damage.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tDamage">
        <xs:annotation>
            <xs:documentation>Details of one unit of damage.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="type" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Code for the type of damage. Validated against the Damage Types
                    reference table
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="type-description" type="xs:string"/>
        <xs:attribute name="component" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Code to identify the part of the unit that is damaged (eg DOOR).
                    Validated against Damage Components reference table.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="component-description" type="xs:string"/>
        <xs:attribute name="location" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Damage Location - a free form String (eg Left Top panels 1 through 3).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="reported-date" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Damage Location - a free form String (eg Left Top panels 1 through 3).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="repaired-date" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation>Damage Location - a free form String (eg Left Top panels 1 through 3).
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="severity" use="required">
            <xs:annotation>
                <xs:documentation>Code to identify the severity of damage - values as specified
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="NONE"/>
                    <xs:enumeration value="MINOR"/>
                    <xs:enumeration value="MAJOR"/>
                    <xs:enumeration value="REPAIRED"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="tHazards">
        <xs:annotation>
            <xs:documentation>Encapsulates all hazardous content of the inventory unit.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="hazard" type="argo:tHazard" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Details of one item of hazardous content.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tHazardItemPlacards">
        <xs:annotation>
            <xs:documentation>Encapsulates all placards of this hazard item.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="hazard-item-placard" type="argo:tHazardItemPlacard" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>One placard Details of hazard item content.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tHazardItemPlacard">
        <xs:annotation>
            <xs:documentation>One placard Details of hazard item content.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="placard" minOccurs="0" type="argo:tPlacard" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>One placard Details of hazard item content.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="description" type="xs:string" use="optional"/>
    </xs:complexType>
    <xs:complexType name="tHazard">
        <xs:annotation>
            <xs:documentation>Details of one item of hazardous content.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="hazard-item-placards" type="argo:tHazardItemPlacards" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="risks" type="argo:tRisks" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="imdg" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>IMDG code for the hazard</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="imdg-description" type="xs:string"/>
        <xs:attribute name="un" use="optional">
            <xs:annotation>
                <xs:documentation>UN Code for the hazard</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:length value="4"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="ltd-qty-flag" type="xs:string" use="optional"/>
        <xs:attribute name="package-type" type="xs:string" use="optional"/>
        <xs:attribute name="inhalation-zone" type="xs:string" use="optional"/>
        <xs:attribute name="explosive-class" type="xs:string" use="optional"/>
        <xs:attribute name="page-number" type="xs:string" use="optional"/>
        <xs:attribute name="flash-point" type="xs:decimal" use="optional"/>
        <xs:attribute name="tech-name" type="xs:string" use="optional"/>
        <xs:attribute name="proper-name" type="xs:string" use="optional"/>
        <xs:attribute name="ems-nbr" type="xs:string" use="optional"/>
        <xs:attribute name="mfag" type="xs:string" use="optional"/>
        <xs:attribute name="packing-group" type="xs:string" use="optional"/>
        <xs:attribute name="packing-group-description" type="xs:string"/>
        <xs:attribute name="haz-id-upper" type="xs:string" use="optional"/>
        <xs:attribute name="substance-lower" type="xs:string" use="optional"/>
        <xs:attribute name="weight-kg" type="xs:decimal" use="optional"/>
        <xs:attribute name="planner-ref" type="xs:string" use="optional"/>
        <xs:attribute name="quantity" type="xs:integer" use="optional"/>
        <xs:attribute name="move-method" type="xs:string" use="optional"/>
        <xs:attribute name="secondary-imo-1" type="xs:string" use="optional"/>
        <xs:attribute name="secondary-imo-2" type="xs:string" use="optional"/>
        <xs:attribute name="deck-restrictions" type="xs:string" use="optional"/>
        <xs:attribute name="marine-pollutants" type="xs:string" use="optional"/>
        <xs:attribute name="dc-lg-ref" type="xs:string" use="optional"/>
        <xs:attribute name="emergency-telephone" type="xs:string" use="optional"/>
        <xs:attribute name="ems-response-guide-nbr" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>The Emergency Response Guide Number that defines how accidents need to be handled.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="notes" type="xs:string" use="optional"/>
        <xs:attribute name="fire-code" type="xs:string" use="optional"/>
        <xs:attribute name="placard-optional" type="xs:string" use="optional"/>
        <!--2009-09-02 pmelappalyam ver2.0.H ARGO-6020 Adding the number-type entity as child to the hazard entity-->
        <xs:attribute name="haz-nbr-type" type="argo:tHazardNumberType" use="optional">
            <xs:annotation>
                <xs:documentation>Hazard Number Type.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="tHazardousGoods">
        <xs:annotation>
            <xs:documentation>
                Hazardous goods reference data
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="hazard" type="argo:tHazard" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="tRisks">
        <xs:annotation>
            <xs:documentation>
                Describes the subsidiary risk associated with hazardous goods
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="hazard-item-placards" type="argo:tHazardItemPlacards" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="risk" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>
                    class of the associated risk
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="placard-name" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Name of the placard associated to this risk
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="placard-optional" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Is the placard associated to this risk optional?
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="tOog">
        <xs:annotation>
            <xs:documentation>Specifies over-dimension attributes of the inventory unit.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="top-cm" type="xs:long" use="optional">
            <xs:annotation>
                <xs:documentation>Centimeters by which the unit is overhigh.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="front-cm" type="xs:long" use="optional">
            <xs:annotation>
                <xs:documentation>Centimeters by which the unit is overlong in the front.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="back-cm" type="xs:long" use="optional">
            <xs:annotation>
                <xs:documentation>Centimeters by which the unit is overlong in the back.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="left-cm" type="xs:long" use="optional">
            <xs:annotation>
                <xs:documentation>Centimeters by which the unit is overwide on the left.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="right-cm" type="xs:long" use="optional">
            <xs:annotation>
                <xs:documentation>Centimeters by which the unit is overwide on the right.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="tPosition">
        <xs:annotation>
            <xs:documentation>Identifies the current location of the inventory unit.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="loc-type" use="required">
            <xs:annotation>
                <xs:documentation>Code to identify the type of position that the inventory unit
                    currently occupies
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="YARD"/>
                    <xs:enumeration value="VESSEL"/>
                    <xs:enumeration value="TRUCK"/>
                    <xs:enumeration value="RAILCAR"/>
                    <xs:enumeration value="TRAIN"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="location" use="optional">
            <xs:annotation>
                <xs:documentation>Code for the general location of the inventory unit. This depends
                    on the type field above. If the type refers to a carrier then this specifies the
                    carrier - ie the vessel or train that the unit is currently on. If the unit is
                    in the yard then this is the code for the yard. If it is a to-come by road, then
                    'loc-type="TRUCK"' should be specified and the 'location' can be omitted.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="slot" use="optional">
            <xs:annotation>
                <xs:documentation>Specific position of the unit within the general location
                    specified above. For example the code identifying an individual container slot
                    on a vessel or in the yard.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="orientation" use="optional">
            <xs:annotation>
                <xs:documentation>Code to describe the orientation of the unit within the slot
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="carrier-id" use="optional">
