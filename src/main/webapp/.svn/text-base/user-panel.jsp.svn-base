<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>

<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />

<div id="user_admin_container" class="panel_container user_admin_container">
	<form id="user_form">
		<fieldset>
			<legend>Info</legend>
			
			<label><spring:message code='Username' />:</label>
			<input class="" id="username" name="username" type="text">
			<br/>
			
			<label><spring:message code='UserN4' />:</label>
			<input class="" id="userN4Name" name="userN4Name" type="text">
			<br/>

			<label><spring:message code='Password' />:</label>
			<input class="" id="password" name="password" type="password">
			<br/>

			<label><spring:message code='Confirm_Password' />:</label>
			<input class="" id="confPassword" name="confPassword" type="password">
			<br/>

			<label><spring:message code='first_Name' />:</label>
			<input class="" id="firstName" name="firstName" type="text">
			<br/>

			<label><spring:message code='last_Name' />:</label>
			<input class="" id="lastName" name="lastName" type="text">
			<br/>

			<label><spring:message code='Email' />:</label>
			<input class="" id="email" name="email" type="text">
			<br/>

			<label><spring:message code='Status' />:</label>
			<select class="" id="statusId" name="statusId">
			</select>
			<br/>

			<label><spring:message code='Company' />:</label>
			<select class="" id="empresaId" name="empresaId" >
			</select>
			<br/>

			<label><spring:message code='Profile' />:</label>
			<select class="" id="profileId" name="profileId" >
			</select>
			<br/>
			
			<label><spring:message code='Report_level' />:</label>
			<select class="" id="reportLevelId" name="reportLevelId">
			</select>
			<br/>
									
			<input class="" id="userN4Id" name="userN4Id" type="hidden">
		</fieldset>

		<br/>		
		<div align="center" id="errorContainer" class="errorContainer"></div>
	</form>
	
	<div align=center>
		<input id=saveButton type=submit value="<spring:message code='Accept' />">
		<input id=closeButton type=button value="<spring:message code='Cancel' />">
	</div>
</div>